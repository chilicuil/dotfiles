Host *
    #default environment
    SendEnv LANG LC_*
    HashKnownHosts yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials no
    #connection sharing
    ControlMaster auto
    ControlPath /tmp/ssh_mux_%h_%p_%r
    ControlPersist 3h
    #use local keys on remote machines, http://git.io/vO2A6
    #ForwardAgent yes
    #keep the connection alive for finnecky servers
    TCPKeepAlive no
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host *.pagekite.me
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host h*.javier.io
    User javier
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host *.javier.io *.minos.io
    User admin

Host bitbucket.org
    IdentityFile ~/.ssh/correo.io.id_rsa

Host *.solucion-it.com solucion-it.com
    User root
    Port 2324
